<%= form_for :user  do |f| %>
  <%= f.collection_select :id, User.all, :id, :name %><br>
  <br>
  <%= f.label             :name           %>
  <%= f.text_field        :name           %><br>
  <%= f.label             :password       %>
  <%= f.password_field    :password       %><br>
  <%= f.submit "Sign In" %>
<% end %>